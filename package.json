  },
  "homepage": "https://github.com/CoderLim/uniform-props-webpack-plugin#readme",
  "dependencies": {
    "webpack-sources": "^1.0.2"
  },
  "devDependencies": {
    "acorn": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.0",
