{
  "name": "uniform-props-webpack-plugin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js'",
    "build:example": "(cd example/react-test && webpack)",
    "clean": "del-cli dist",
    "test": "npm run build && jest",
    "test:coverage": "npm run build && jest --collectCoverageFrom='src/**/*.js' --coverage",
    "travis:test": "npm run test -- --runInBand",
    "travis:coverage": "npm run test:coverage -- --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoderLim/uniform-props-webpack-plugin.git"
  },
